/ {
	leds: leds {
		compatible = "gpio-leds";
		red_led: led_1 {
			gpios = <&gpioa 0 GPIO_ACTIVE_LOW>;
			label = "Red LED";
		};
		green_led: led_0 {
			gpios = <&gpioa 3 GPIO_ACTIVE_HIGH>;
			label = "User LD3";
		};
		blue_led: led_2 {
			gpios = <&gpioa 1 GPIO_ACTIVE_LOW>;
			label = "Blue LED";
		};
	};
};
